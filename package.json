{
  "name": "ftoy-cli",
  "version": "1.0.0",
  "description": "The CLI tool for toy-system.",
  "preferGlobal": true,
  "bin": {
    "ftoy": "./bin/ftoy.js"
  },
  "engines": {
    "node": ">=8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChenShihao/ftoy-cli.git"
  },
  "scripts": {
    "build": "tsc",
    "build:w": "tsc -w",
    "pretest": "npm run build",
    "test": "jest",
    "test:e2e": "jest"
  },
  "keywords": [],
  "author": {
    "name": "chenshihao",
    "email": "victor_csh@outlook.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/debug": "0.0.31",
    "@types/inquirer": "^0.0.43",
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "@types/ora": "^1.3.4",
    "@types/pino": "^5.6.1",
    "@types/yargs": "^12.0.1",
    "debug": "^4.1.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "inquirer": "^6.2.0",
    "ora": "^3.0.0",
    "pino": "^5.8.0",
    "yargs": "^12.0.2"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/lib/",
      "/node_modules/"
    ]
  }
}
